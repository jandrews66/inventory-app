<!DOCTYPE html>
<html>
  <head>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <%- include('./partials/head'); %>
  </head>
  <body>
    <header>
      <%- include('./partials/header'); %>
    </header>
    <h1><%= title %></h1>
    <ul>
        <% brand_list.forEach(function(brand) { %>
            <li>
                <a href="<%= brand.url %>"><strong><%= brand.name %></strong></a> -
                <%= brand.description %>

            </li>
       <% }); %>

    </ul>

  </body>
</html>